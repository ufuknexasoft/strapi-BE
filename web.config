<configuration>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="server.js" verb="*" modules="iisnode" />
    </handlers>
    <rewrite>
      <rules>
        <rule name="NodeJS" enabled="true" stopProcessing="true">
          <match url=".*" />
          <action type="Rewrite" url="http://localhost:1337/{R:0}" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
